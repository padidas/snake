		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.5",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
